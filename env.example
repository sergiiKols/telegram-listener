# Telegram API Credentials
# Получить с https://my.telegram.org/apps
TELEGRAM_API_ID=YOUR_API_ID_HERE
TELEGRAM_API_HASH=YOUR_API_HASH_HERE

# Ваш номер телефона для авторизации
# Формат: +7 код страны номер
TELEGRAM_PHONE=+YOUR_PHONE_HERE

# n8n webhook URL для отправки сообщений
# Оставить пусто если еще не настроили n8n
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/telegram-listener
